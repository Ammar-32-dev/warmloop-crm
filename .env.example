# WarmLoop CRM - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
# Get these from your Supabase project dashboard at https://supabase.com/dashboard
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server-side only (for edge functions/backend)
# DO NOT commit this value - add to GitHub Secrets for CI/CD
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# Application Configuration
NODE_ENV=production
PORT=4173

# Additional Configuration (optional)
# VITE_APP_NAME=WarmLoop CRM
# VITE_API_TIMEOUT=30000
