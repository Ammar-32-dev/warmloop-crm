+            <Link
+              to="/datasets"
+              className="p-2 text-gray-600 hover:bg-gray-100 rounded-lg transition-colors duration-150"
+            >
+              <ArrowLeft className="w-5 h-5" />
+            </Link>
+            <div>
+              <h1 className="text-3xl font-bold text-gray-900" style={{ fontFamily: 'Poppins, sans-serif' }}>
